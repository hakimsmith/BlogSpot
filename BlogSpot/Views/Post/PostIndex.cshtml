@model IEnumerable<BlogSpot.Models.Post>

@{
    Layout = "_Layout";
}

<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Posts</title>
</head>
<body>
    <div>
        <h2> Posts</h2>
    </div>
    <main>

        @foreach (var item in Model)
        {

            <a href="/Post/SinglePost/@item.PostId">


                <h3>@item.PostTitle</h3>
            </a>
        }

    </main>
</body>
</html>
